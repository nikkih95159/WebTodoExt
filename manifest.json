{
    "name": "Todo Extension",
    "version": "0.0.1",
    "description": "A todo list extension",
    "permissions": [],
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "images/get_started48.png",
        "default_title": "Todoext"
    },
    "background": {
      "page": "popup.html",
      "persistent": false
    },
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'; connect-src 'self' wss://*.firebaseio.com;",
    "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
    },
    "manifest_version": 2
}