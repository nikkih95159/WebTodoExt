{
    "name": "Todo Extension",
    "version": "0.0.4",
    "description": "A todo list extension",
    "permissions": ["geolocation"],
    "options_page": "options.html",
    "browser_action": {
        "default_title": "Todoext"
    },
    "background": {
      "page": "popup.html",
      "persistent": false
    },
    "chrome_url_overrides": {
        "newtab": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://api.openweathermap.org/; object-src 'self';",
    "icons": {
        "48": "images/todoextimage.png"
    },
    "manifest_version": 4
}